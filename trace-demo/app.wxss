.container{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 180rpx 20rpx;
  box-sizing: border-box;
}
/* .view{
  height:128rpx;
} */
.icon{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 20rpx;
}
.icon-right{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 25rpx;
  right: 0;
}
.van-nav-bar {
  background: transparent !important;
}
.van-nav-bar__title {
  color: #FFFFFF;
}
.view .van-icon {
  font-size: 48rpx !important;
}

.allbgdimg{
  width: 100%;
  height: 128rpx;
  position: fixed;
  top: 0;
  z-index: -1;
}
.allbgdimg::after{
  content: '';
  position: fixed;
  top: 0;
  z-index: 0;
  width: 100vw;
  height: 128rpx;
  background:rgba(0,0,0,.5);
}
.select_one {
  width: 100%;
  display:inline-block;
}
.return-left{
  width: 50rpx;
  height: 50rpx;
  float: left;
  position: fixed;
  z-index: 999;
  left: 10rpx;
  top: 50rpx;
}
.library-search{
  width: 100%;
  overflow: hidden;
}
.search-icon{
  font-size: 14rpx;
  float: right;
  margin-top: 14rpx;
}
.viewtext {
  margin: 20rpx;
}
.flex-wrp{
  display:flex;
  border-bottom: 1px solid #ccc;
  padding:16rpx 20rpx;
  background: #FFFFFF;
}
.bc_green {
  background: #009688;
}
.bc_light_green {
  background: #5FB878;
}
.bc_red {
  background: #FF5722;
}
.bc_blue {
  background: #1E9FFF;
}
.bc_orange {
  background: #FFB800;
}
.bc_orange_yellow {
  background: #ff7d1f;
}

.panel_top {
  position: fixed;
  flex-direction: column;
}

/* 出库详情页 */
.section {
  /* position: fixed; */
}
.bc_leftbox {
  width: 160rpx;
  font-size: 30rpx;
  padding: 20rpx;
  vertical-align: middle;
  color: #FFFFFF;
  display: flex;
  align-items: center;
  justify-content: center;
}
.bc_rightbox {
  width: 100%;
  padding-left: 20rpx;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction:column;
}
.bc_rightbtn {
  font-size: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction:column;
  float: right;
}
.btn_botton {
  margin-top: 12rpx;
}
.bc_text {
  width: 100%;
  line-height: 38rpx;
  display: inline-block;
}
.bc_right {
  text-align: right;
}
.bc_fontmax {
  font-weight: bold;
}
.scan-code{
  flex-direction: row;
  /* display: flex; */
  padding: 16rpx 20rpx;
  background: #FFFFFF;
}
.scan-code-left {
  display: flex;
  align-items: center;
  justify-content:flex-start;
  flex-direction: row;
  float: left;
  width:68%;
}
.scan-code-left .fontset {
  width: 140rpx;
  font-size: 28rpx;
}
.allborder {
  /* height: 40rpx; */
  align-items: center;
  justify-content: space-between;
  /* border: 1px solid #ccc; */
  /* padding: 10rpx; */
  background: #f1f1f1;
}
.allborderwidth {
  width: 100%;
}
.scan-code-right {
  width: 220rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: row;
  float:right;
}
.product-panel {
  background: #f6f6f6;
}
.product {
  margin: 16rpx 20rpx 0rpx 20rpx;
  flex-direction: column;
  display: flex;
  background: #FFFFFF;
  padding: 20rpx;
  border-radius:5px;
}
.product-top {
  font-size: 40rpx;
  /* font-weight: bold; */
  padding-bottom: 12rpx;
  border-bottom: 2rpx #dddddd dashed;
}
.product-top {
  font-size: 36rpx;
  /* font-weight: bold; */
  padding-bottom: 12rpx;
  border-bottom: 2rpx #dddddd dashed;
}
.panel-center {
  display: flex;
  position: relative;
  background:#f6f6f6;
  overflow-y:auto;
  padding-bottom: 220rpx;
  height: 100%;
}
.product-center {
  flex-direction: row;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 12rpx;
  color: #545454;
}
.product-center .prorow {
  font-size: 24rpx;
  display: flex;
  align-items: center;
  justify-content:flex-start;
  width:50%;
}
.product-bottom {
  font-size: 24rpx;
  flex-direction: row;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  margin-top: 12rpx;
  color: #545454;
}
.product-bottom-left {
  margin-left: 12rpx;
}

.panel-abc {
  position: fixed;
  bottom: 0;
  width:100%;
  background: #FFFFFF;
}
.panel-bottom {
  position: fixed;
  right:0;
  bottom: 0;
  width:100%;
  background: #FFFFFF;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  z-index: 0;
}

.btn-all{
  width: auto;
  height: 38px;
  border: 0;
  line-height: 28px;
  color: #fff;
  font-size: 14px;
  border-radius: 0;
  padding: 12rpx 60rpx;
  margin: 0;
}
.btn-scan-code{
  font-size: 100rpx;
  position: fixed;
  left: 20rpx;
  bottom: 10rpx;
  color: #FFB800;
}

/* 弹出框 windows */
.back_scan {
  padding: 10rpx;
  float:left;
  background: #FFB800;
  border-radius: 0px;
  margin-top: 20rpx;
  margin-left: 20rpx;
  margin-bottom: 20rpx;
  text-align: center;
  vertical-align: middle;
  align-items: center;
  justify-content: center;
}

.van-search {
  padding: 20rpx !important;
  /* padding-bottom: 20rpx !important;
  padding-left: 0rpx !important;
  padding-right: 20rpx !important; */
}

.van-notice-bar {
  display:-webkit-flex;
  display:flex;
  height:40rpx !important;
  padding:12rpx !important;
  line-height:24px;
}

.van-cell__value {
  flex: 3 !important;
}

.van-search__content {
  padding-left: 0 !important;
  /* margin-left: 20rpx; */
}
.van-popup {
  overflow-y: hidden !important;
}
